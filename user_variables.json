{
    "remove": {
        "regex": "s/[:|?\"]//g"
    },
    "base_paths": {
        "origin_base": ".",
        "ancillary_copying_base": ".",
        "destinations_base": "."
    },
    "origin": {
        "path": "origin/", 
        "path_type": "relative",
        "destinations":[
            {"path": "testdest1", "path_type": "relative"},
            {"path": "testdest2", "path_type": "relative"},
            {"path": "testdest3", "path_type": "relative"},
            {"path": "testdest4", "path_type": "relative"}
        ],
        "use_batch": "True"
    },
    "ancillary_copying": [
        {
           "src_path": "other_origin/", 
           "src_path_type": "relative",
           "dest_path": "origin/other_folder/", 
           "dest_path_type": "relative" 
        },
        "use_batch": "False"
    ],
    {"commands": 
        [  
            {"rsync_command": "rsync",
            "arguments": 
                [
                "flags": "-auv", 
                "delete_after": "--delete-after", 
                "write_batch": "--write-batch=batch"
                ],
            "symbolic_arguments":
                [   
                "source",
                "destination"
                ]
            }, 
            {"batch_command": "bash", 
                "arguments":
                    [
                    "batch.sh"
                    ],
                "symbolic_arguments":
                    [
                    "destination"
                    ]    
            }
        ]
     }
